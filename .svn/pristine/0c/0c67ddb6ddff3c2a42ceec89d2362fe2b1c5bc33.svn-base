package com.neusoft.hostpital.vo;

import java.util.List;



public class Drugs {

	private int drugs_id;
	private String drugs_no;
	private String drugs_name;
	private String drugs_type;
	private Double drugs_price;
	private int project_id;
	private List<Project> project;
	public List<Project> getProject() {
		return project;
	}

	public void setProject(List<Project> project) {
		this.project = project;
	}

	public int getProject_id() {
		return project_id;
	}

	public Drugs(int drugs_id, String drugs_no, String drugs_name, String drugs_type, Double drugs_price,
			int project_id) {
		super();
		this.drugs_id = drugs_id;
		this.drugs_no = drugs_no;
		this.drugs_name = drugs_name;
		this.drugs_type = drugs_type;
		this.drugs_price = drugs_price;
		this.project_id = project_id;
	}

	public void setProject_id(int project_id) {
		this.project_id = project_id;
	}

	public int getDrugs_id() {
		return drugs_id;
	}

	public void setDrugs_id(int drugs_id) {
		this.drugs_id = drugs_id;
	}

	public String getDrugs_no() {
		return drugs_no;
	}

	public void setDrugs_no(String drugs_no) {
		this.drugs_no = drugs_no;
	}

	public String getDrugs_name() {
		return drugs_name;
	}

	public void setDrugs_name(String drugs_name) {
		this.drugs_name = drugs_name;
	}

	public String getDrugs_type() {
		return drugs_type;
	}

	public void setDrugs_type(String drugs_type) {
		this.drugs_type = drugs_type;
	}

	public Double getDrugs_price() {
		return drugs_price;
	}

	public void setDrugs_price(Double drugs_price) {
		this.drugs_price = drugs_price;
	}

	@Override
	public String toString() {
		return "Drugs [drugs_id=" + drugs_id + ", drugs_no=" + drugs_no + ", drugs_name=" + drugs_name + ", drugs_type="
				+ drugs_type + ", drugs_price=" + drugs_price + ", project_id=" + project_id + "]";
	}

	public Drugs(int drugs_id, String drugs_no, String drugs_name, String drugs_type, Double drugs_price) {
		super();
		this.drugs_id = drugs_id;
		this.drugs_no = drugs_no;
		this.drugs_name = drugs_name;
		this.drugs_type = drugs_type;
		this.drugs_price = drugs_price;
	}

	public Drugs() {
		super();
	}

}
