<%@ page language="java" contentType="text/html; charset=utf-8"
	pageEncoding="utf-8"%>
<%
	String path = request.getContextPath();
	String basePath = request.getScheme() + "://" + request.getServerName() + ":" + request.getServerPort()
			+ path + "/";
%>

<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<base href="<%=basePath%>">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<title></title>
<meta name="keywords" content="" />
<meta name="description" content="" />
<meta name="generator" content="" />
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta name="viewport"
	content="width=device-width; initial-scale=1.0; maximum-scale=1.0; user-scalable=0;">
<link href="css/haiersoft.css" rel="stylesheet" type="text/css"
	media="screen,print" />
<link href="css/print.css" rel="stylesheet" type="text/css"
	media="print" />
<script src="js/jquery-1.10.1.min.js" type="text/javascript"></script>
<script src="js/side.js" type="text/javascript"></script>

<!--[if lt IE 9]>
<script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
<script src="http://css3-mediaqueries-js.googlecode.com/svn/trunk/css3-mediaqueries.js"></script>
<![endif]-->
</head>


<script type="text/javascript">
	function selectAll() {
		location.href = "<%=basePath%>pro_selectAll";
		
	}
	function selectOne() {
		location.href = "<%=basePath%>pro_selectOne";
		
	}
	function pro_add() {
		location.href = "<%=basePath%>pro_add1";

	}

	function checkProjectNo() {
		var no = $("#prono").val();
		$.ajax({
			type : "get",
			url : "isExistProjectNo?projectNo=" + no,
			async : true,
			success : function(data) {
			if(data != ''){
			document.getElementById("isExistNo").innerHTML = "编号已存在，请修改！！";
			}else{
			document.getElementById("isExistNo").innerHTML = "";
			}
			}
		});
	}
	
		function checkProjectName() {
		var no = $("#proname").val();
		$.ajax({
			type : "get",
			url : "isExistProjectName?projectName=" + no,
			async : true,
			success : function(data) {
			if(data != ''){
			document.getElementById("isExistName").innerHTML = "名称已存在，请修改！！";
			}else{
			document.getElementById("isExistName").innerHTML = "";
			}
			}
		});
	}
	
	function cknull(){
		var prono = $("#prono").val();
		var proname = $("#proname").val();
		var prolevel = $("#prolevel").val();
		if(prono==''||proname==''||prolevel==''){
	         alert("全部不能为空！！！");
		}else if(prolevel!='1'&&prolevel!='2'&&prolevel!='3'&&prolevel!='4'){
			alert("项目等级必须在1~4之间");
		}else{
			 document.getElementById("SBBBBB").submit();
		}
	}

	
</script>

<body>

	<!-- Popup -->
	<div id="Popup">

		<!-- SubPopup -->
		<div id="SubPopup">
			<script type="text/javascript">
				$(
						function() {
							$(".select").each(
									function() {
										var s = $(this);
										var z = parseInt(s.css("z-index"));
										var dt = $(this).children("dt");
										var dd = $(this).children("dd");
										var _show = function() {
											dd.slideDown(200);
											dt.addClass("cur");
											s.css("z-index", z + 1);
										};
										var _hide = function() {
											dd.slideUp(200);
											dt.removeClass("cur");
											s.css("z-index", z);
										};
										dt.click(function() {
											dd.is(":hidden") ? _show()
													: _hide();
										});
										dd.find("a").click(function() {
											dt.html($(this).html());
											_hide();
										});
										$("body").click(
												function(i) {
													!$(i.target).parents(
															".select").first()
															.is(s) ? _hide()
															: "";
												});
									})
						})
			</script>
			<form action="pro_add1" method="post" id = "SBBBBB">
				<div class="form_boxC">
					<p>
						"<span class="f_cB">*</span>"号为必填项目,项目等级为1、2、3、4
					</p>
					<table cellpadding="0" cellspacing="0" style="position: relative;top: 40px">
						<tr>
							<th width="120">诊疗项目编号 <span class="f_cB">*</span></th>
							<td><div class="txtbox floatL" style="width: 200px;">
									<input id="prono" onblur="checkProjectNo()" onkeyup="this.value=this.value.replace(/[^\w]/g,'')" onpaste="this.value=this.value.replace(/[^\w]/g,'')" name="projectno" type="text" size="5" value=""
										style="width: 180px; height: 35px;" />
								</div></td>
								<span id = "isExistNo" style="color:red;font-size:18px;position: relative;top: 75px;left: 370px"></span>
						</tr>
						<tr>
							<th>诊疗项目名称 <span class="f_cB">*</span></th>

							<td><div class="txtbox floatL" style="width: 100px;">
							
									<input id="proname" onblur="checkProjectName()" name="projectname" type="text" size="5" value=""
									onkeyup="this.value=this.value.replace(/(^\s+)|(\s+$)/g,'');"	style="width: 80px;" />
										
								</div></td>
								<span id = "isExistName" style="color:red "></span>

						</tr>
						<tr>
							<th>诊疗项目等级<span class="f_cB">*</span></th>

							<td><div class="txtbox floatL" style="width: 100px;">
									<input id="prolevel" name="projectlevel" type="text" size="5" value=""
										style="width: 80px;" />
								</div></td>
							</div>
							</td>
							<td>
								<div style="position: relative; left: 1000px; top: -200px">

									<div class="btn_box floatR">
										<input type="button" onclick="cknull()" value="下一步"
											onmousemove="this.className='input_move'"
											onmouseout="this.className='input_out'">
									</div>
								</div>
							</td>
						</tr>
						</form>
</body>
</html>